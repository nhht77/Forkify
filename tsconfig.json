{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es5",
      "jsx": "react",
      "lib":["es2015", "es6", "dom", "es5"],
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      // Restrict DOM return null behavior by querySelector
      // "strictNullChecks": true,
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules",
        "**/*.spec.ts"
    ]
  }